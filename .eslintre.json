{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "quotes": "off",
        "object-curly-newline": "off",
        "no-trailing-spaces": "off",
        "max-len": "off"
    }
}